{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["./public/home.bundle.js"],
  "rules": {
    "no-debugger": 2,
    "no-duplicate-imports": 2,
    "no-func-assign": 2,
    "camelcase": 2,
    "no-empty": 2
  }
}
